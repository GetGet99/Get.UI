<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:guis="using:Get.UI.Controls"
    xmlns:tabs="using:Get.UI.Controls.Tabs"
    xmlns:ctns="using:Get.UI.Controls.Containers"
>
    <Style x:Name="DefaultOffsetElementStyle" TargetType="guis:OffsetElement">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="guis:OffsetElement">
                    <Border x:Name="RootElement"/>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="guis:OffsetElement" BasedOn="{StaticResource DefaultOffsetElementStyle}"/>
</ResourceDictionary>
